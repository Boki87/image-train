{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client", "node"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve"
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts", "**/*.tsx", "**/*.js", "src/store/index.jsx", "src/main.jsx", "src/App.jsx", "src/utils/index.jsx", "src/components/PresetCard.jsx", "src/components/NewPresetPage.jsx", "src/components/HomePage.jsx", "src/components/AddNewPresetButton.jsx", "src/store/PresetsProvider.jsx"]
}
